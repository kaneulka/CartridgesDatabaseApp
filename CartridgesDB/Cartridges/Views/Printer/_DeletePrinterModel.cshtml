@model PrinterModelViewModel
@using Cartridges.Web.Models.ViewModel
@using Cartridges.Web.Models

<form asp-action="DeletePrinterModel" role="form">
    @Html.Partial("_ModalHeader", new ModalHeader { Heading = "Удалить модель?" })

    <div class="modal-body">
        <input asp-for="Id" />
        <input asp-for="PrinterCompanyId"  type="hidden"/>
        Вы уверены, что хотите удалить @Model.Name?
    </div>
    @Html.Partial("_ModalFooter", new ModalFooter { SubmitButtonText = "Удалить" })
</form>